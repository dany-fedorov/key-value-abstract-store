{
  "name": "key-value abstract store",
  "version": "0.0.1",
  "description": "Very abstract nested key-value store.",
  "main": "./lib/index.js",
  "scripts": {
    "test": "NODE_OPTIONS='--stack-trace-limit=1000' ./node_modules/.bin/jest",
    "build": "tsc -p ./tsconfig.build.json",
    "prepare": "rm -rf ./dist && npm run build",
    "build-watch": "chokidar './src/**/*.ts' -c 'npm run build' -i './node_modules/**/*' --initial"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dany-fedorov/kvas.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "key-value interface",
    "key-value abstract map",
    "key-value async store",
    "key-value nested store"
  ],
  "author": "Dany Fedorov",
  "license": "MIT",
  "devDependencies": {
    "@jest/types": "^28.1.3",
    "@tsconfig/node16-strictest": "^1.0.3",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.7.3",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "chokidar": "^3.5.3",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "^28.1.2",
    "prettier": "2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
